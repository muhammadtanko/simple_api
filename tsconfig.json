{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
     "outDir": "lib",
      "sourceMap": true,
      "strict": true,
      "target": "es2017",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "lib": ["es5", "es6", "dom", "dom.iterable"]
    },
    "compileOnSave": true,
    "include": [
      "src"
    ]
  }
  
